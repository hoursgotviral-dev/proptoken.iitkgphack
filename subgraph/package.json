{
    "name": "proptoken-rwa-subgraph",
    "version": "1.0.0",
    "description": "The Graph subgraph for RWA tokenization platform",
    "scripts": {
        "codegen": "graph codegen",
        "build": "graph build",
        "deploy": "graph deploy --product hosted-service yourorg/proptoken-rwa",
